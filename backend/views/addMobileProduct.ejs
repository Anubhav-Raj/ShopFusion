<script>
  (function () {
    "use strict";

    // Fetch all the forms we want to apply custom Bootstrap validation styles to
    var forms = document.querySelectorAll(".needs-validation");

    // Loop over them and prevent submission
    Array.prototype.slice.call(forms).forEach(function (form) {
      form.addEventListener(
        "submit",
        function (event) {
          if (!form.checkValidity()) {
            event.preventDefault();
            event.stopPropagation();
          }

          form.classList.add("was-validated");
        },
        false
      );
    });
  })();

  function replicateForm() {
    // Get the form container
    var formContainer = document.querySelector(".formContainer");

    // Clone the form content
    var clonedForm = formContainer.cloneNode(true);

    // Append the cloned form to the container
    formContainer.parentNode.appendChild(clonedForm);
  }

  function removeForm() {
    // Get the form container
    var formContainer = document.querySelector(".formContainer");

    // Check if there is more than one form
    if (formContainer.parentNode.children.length > 1) {
      // Remove the last form
      formContainer.parentNode.removeChild(formContainer.parentNode.lastChild);
    }
  }

