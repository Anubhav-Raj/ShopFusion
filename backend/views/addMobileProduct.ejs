        <div id="subjects-container${studentCounter}"></div>
         <hr>
        
  `;

    studentsContainer.appendChild(studentDiv);
  }

  function removeStudent(studentElement) {
    const studentsContainer = document.getElementById("students-container");
    studentsContainer.removeChild(studentElement);
  }

  function addSubject(studentId) {
    const subjectsContainer = document.getElementById(
      `subjects-container${studentId}`
    );
    const subjectDiv = document.createElement("div");
    subjectDiv.classList.add("subject-container");

    subjectDiv.innerHTML = `

