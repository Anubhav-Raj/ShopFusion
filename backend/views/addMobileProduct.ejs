          <line x1="6" y1="6" x2="18" y2="18"></line>
        </svg>
      </div>

      </div>
  
  `;

    subjectsContainer.appendChild(subjectDiv);
  }

  function removeSubject(studentId) {
    const subjectsContainer = document.getElementById(
      `subjects-container${studentId}`
    );
    const subjects =
      subjectsContainer.getElementsByClassName("subject-container");
    if (subjects.length > 1) {
      subjectsContainer.removeChild(subjects[subjects.length - 1]);
    }
  }

  function submitForm() {
    const studentsContainer = document.getElementById("students-container");
    const students =
      studentsContainer.getElementsByClassName("student-container");
    const formData = [];

    for (let i = 0; i < students.length; i++) {
      const student = students[i];
      const studentData = {
        name: document.getElementById(`name${i + 1}`).value,
        class: document.getElementById(`class${i + 1}`).value,
        age: document.getElementById(`age${i + 1}`).value,
        subjects: [],
      };

      const subjectsContainer =
        student.getElementsByClassName("subject-container");
      for (let j = 0; j < subjectsContainer.length; j++) {
        const subject = subjectsContainer[j];
        const subjectData = {
          subject: subject.querySelector('input[placeholder="Subject"]').value,
          marks: subject.querySelector('input[placeholder="Marks"]').value,
        };
        studentData.subjects.push(subjectData);
      }

      formData.push(studentData);
    }

    console.log(formData);
